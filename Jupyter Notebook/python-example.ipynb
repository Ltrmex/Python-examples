   "source": [
    "# Python Program to find the L.C.M. of two input number\n",
    "def lcm(a, b):\n",
    "    # Calculate product of a and b\n",
    "    m = a* b\n",
    "    \n",
    "    # Calculate GCD first\n",
    "    while b > 0:\n",
    "        a, b = b, (a % b)\n",
    "        \n",
    "    # LCM is product divided by GCD\n",
    "    return m / a"
   ]
  },
